{
  "dsccViz": {
    "gcsDevBucket": "gs://yl-viz-dev/sunburst",
    "gcsProdBucket": "gs://public-community-viz-showcase-reports/sunburst",
    "jsFile": "index.js",
    "jsonFile": "index.json",
    "cssFile": "index.css",
    "print": "printMessage.js"
  },
  "scripts": {
    "build:dev": "ts-node scripts/build.ts dev",
    "build:prod": "ts-node scripts/build.ts prod",
    "push:dev": "dscc-scripts viz push -d dev",
    "push:prod": "dscc-scripts viz push -d prod",
    "update_message": "dscc-scripts viz update_message -f object",
    "start": "tsc -w & webpack-dev-server",
    "test": "jest",
    "prettier": "prettier --write \"{src,test}/**/*.{ts,js}\"",
    "tsc:check": "tsc --noEmit"
  },
  "devDependencies": {
    "@google/dscc": "^0.3.8",
    "@google/dscc-scripts": "^1.0.10",
    "@types/bluebird": "^3.5.27",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/mz": "^0.0.32",
    "@types/open": "^6.1.0",
    "jest": "^24.8.0",
    "mz": "^2.7.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.2",
    "d3": "^5.9.2",
    "d3-array": "^2.2.0",
    "d3-hierarchy": "^1.1.8",
    "d3-scale-chromatic": "^1.3.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
